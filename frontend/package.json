  "scripts": {
    "analyze": "STATS=true webpack --config ./webpack/webpack.config.client.production.babel.js --profile --json > stats.json",
    "dev": "rm -r static; run-p dev:client:build dev:server:build dev:server:start",
    "prod": "run-s prod:client:build prod:server:build prod:server:start",
    "dev:client:build": "NODE_ENV='development' node ./webpack/webpack-dev-server.mjs",
    "dev:server:build": "NODE_ENV='development' webpack --config ./webpack/webpack.config.server.development.mjs --watch",
    "prod:client:build": "NODE_ENV='production' webpack --config ./webpack/webpack.config.client.production.mjs",
    "prod:server:build": "NODE_ENV='production' webpack --config ./webpack/webpack.config.server.production.mjs",
    "dev:server:start": "NODE_ENV='development' nodemon ./bin/server.mjs --watch ./static/server",
    "prod:server:start": "NODE_ENV='production' pm2 start ./bin/server.mjs -i 0 --no-daemon --name 'frontend'",
    "test": "jest --watch --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|node_modules|static)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/config/jest/fileTransform.js",
      ".+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
