    "whatwg-fetch": "1.0.0"
  },
  "engines": {
    "node": ">=6.9 <=8.2.1"
  },
  "scripts": {
    "analyze": "NODE_ENV=\"production\" webpack --config ./webpack/prod.config.js --json | webpack-bundle-size-analyzer",
    "stats": "NODE_ENV=\"production\" webpack --config ./webpack/prod.config.js --json --profile > stats.json",
    "build": "better-npm-run build",
    "test": "jest --watch --env=jsdom",
    "prod": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "docker-dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-docker-dev\"",
    "docker-prod": "concurrently --kill-others \"npm run start-docker-prod\"",
    "watch-client": "better-npm-run watch-client",
    "start-dev": "better-npm-run start-dev",
    "start-docker-dev": "better-npm-run start-docker-dev",
    "start-docker-prod": "better-npm-run start-docker-prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "INTERNAL_HOST": "localhost",
        "INTERNAL_PORT": "8089"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "INTERNAL_HOST": "localhost",
        "INTERNAL_PORT": "8089"
      }
    },
    "start-docker-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "INTERNAL_HOST": "nginx",
        "INTERNAL_PORT": "80"
      }
    },
    "start-docker-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "INTERNAL_HOST": "nginx",
        "INTERNAL_PORT": "80"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|node_modules|static)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/config/jest/fileTransform.js",
      ".+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
