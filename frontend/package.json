  "scripts": {
    "analyze": "webpack --mode production --config ./webpack/webpack.config.client.production.babel.js --json",
    "build-player-dev": "better-npm-run build-player-dev",
    "build-player": "better-npm-run build-player",
    "delay": "node ./node_modules/npm-delay 30000",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "docker-dev": "run-p docker-dev:client:build dev:server:prepare docker-dev:server:build docker-dev:server:delay",
    "docker-prod": "run-s prod:client:build prod:server:build start-docker-prod",
    "docker-dev:client:build": "babel-node webpack/webpack-dev-server.js",
    "docker-dev:server:build": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "docker-dev:server:delay": "run-s delay docker-dev:server:start",
    "docker-dev:server:start": "better-npm-run start-docker-dev",
    "prod:client:build": "webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "start-docker-dev": "better-npm-run start-docker-dev",
    "start-docker-prod": "better-npm-run start-docker-prod",
    "stats": "NODE_ENV=\"production\" webpack --config ./webpack/webpack.config.client.production.babel.js --json --profile > stats.json",
    "test": "jest --watch --env=jsdom"
  },
  "betterScripts": {
    "build-player-dev": {
      "command": "webpack --watch --verbose --colors --display-error-details --config webpack/player.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-player": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/player.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-docker-dev": {
      "command": "nodemon ./bin/server.js --watch ./static",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-docker-prod": {
      "command": "pm2 start ./bin/server.js -i 0 --no-daemon --name 'frontend'",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|node_modules|static)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/config/jest/fileTransform.js",
      ".+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
