    },
    "modulePaths": [
      "src"
    ],
    "scriptPreprocessor": "<rootDir>/config/jest/transform.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
